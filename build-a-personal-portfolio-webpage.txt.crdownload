/* Basic styling for your personal portfolio */
body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
}

#navbar {
    background-color: #333;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1000;
}

#navbar ul {
    list-style: none;
    display: flex;
    justify-content: space-around;
    padding: 10px;
    margin: 0;
}

#navbar a {
    text-decoration: none;
    color: #fff;
    font-size: 18px;
}

#welcome-section {
    margin-top: 60px;
    height: 100vh;
    background-image: url("your-background-image.jpg");
    background-size: cover;
    color: #fff;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}

#projects {
    padding: 40px;
}

.project-tile {
    background-color: #f5f5f5;
    border: 1px solid #ddd;
    padding: 20px;
    margin: 20px;
    border-radius: 5px;
}

#profile-link {
    color: #007BFF;
}

/* Media query for responsive design */
@media (max-width: 768px) {
    #navbar {
        position: fixed;
    }
    #navbar ul {
        flex-direction: column;
        text-align: center;
    }
    #navbar a {
        display: block;
        padding: 10px;
    }
}


